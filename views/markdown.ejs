<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/bootstrap/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="/stylesheets/github-markdown.css" type="text/css" />
    <link rel="stylesheet" href="/stylesheets/style.css" type="text/css" />
    <script src="http://cdn.bootcss.com/jquery/1.11.1/jquery.min.js"></script>
    <script src="/bootstrap/js/bootstrap.min.js"></script>
  </head>
  <body>
    <div class="row">
      <div class="col-md-1"></div>
      <div class="col-md-2 col-sm-4 col-xs-5">
          <ul class="mdList">
            <% for(var i=0; i<mdFiles.length; i++) { 
               var className = (mdFiles[i] == fileName)? 'itemHighlight' : ''; 
            %>
            <li class='<%= className %>'>
               <a href='<%= mdFiles[i] %>'><%= mdFiles[i] %></a>
            </li>
            <% } %>
          </ul>
      </div>
      <div class="col-md-8 col-sm-8 col-xs-7 markdown-body mdContent"><%- content %></div>
      <div class="col-md-1"></div>
   </div>
  </body>
</html>
